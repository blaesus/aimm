{
    "name": "aimm",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "start": "sh dev.sh",
        "deploy": "sh ops/deploy.sh",
        "deploy:nginx": "sh ops/deploy_nginx.sh",
        "build:web": "node_modules/.bin/webpack --config client/web/webpack.config.prod.js",
        "mirror": "ts-node -r tsconfig-paths/register src/server/data/mirror.ts",
        "genprisma": "./node_modules/.bin/prisma generate",
        "migrateprisma": "./node_modules/.bin/prisma migrate dev --name"
    },
    "prisma": {
        "schema": "data/schema.prisma"
    },
    "dependencies": {
        "@aws-sdk/client-s3": "^3.329.0",
        "@aws-sdk/lib-storage": "^3.329.0",
        "agentkeepalive": "^4.3.0",
        "axios": "^1.4.0",
        "dotenv": "^16.0.3",
        "html-webpack-inline-source-plugin": "^0.0.10",
        "koa": "^2.14.2",
        "koa-bodyparser": "^4.4.0",
        "koa-router": "^12.0.0",
        "prisma": "^4.13.0",
        "react": "18.2.0",
        "react-dom": "18.2.0"
    },
    "devDependencies": {
        "@prisma/client": "^4.13.0",
        "@types/koa": "^2.13.6",
        "@types/koa-bodyparser": "^4.3.10",
        "@types/koa-router": "^7.4.4",
        "@types/node": "18.16.2",
        "@types/pg": "^8.6.6",
        "@types/react": "18.2.5",
        "@types/react-dom": "18.2.3",
