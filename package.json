        "html-webpack-plugin": "^5.5.1",
        "mini-css-extract-plugin": "^2.7.5",
        "nodemon": "^2.0.22",
        "terser-webpack-plugin": "^5.3.8",
        "ts-loader": "^9.4.2",
        "ts-node": "^10.9.1",
        "tsconfig-paths": "^4.2.0",
        "typescript": "5.0.4",
        "webpack": "^5.82.1",
        "webpack-cli": "^5.1.1",
        "webpack-dev-server": "^4.15.0"
    }
